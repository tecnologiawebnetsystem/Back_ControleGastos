        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "Usuario": {
        "type": "object",
        "properties": {
          "UsuarioID": {
            "type": "integer"
          },
          "Nome": {
            "type": "string"
          },
          "Email": {
            "type": "string"
          },
          "Senha": {
            "type": "string"
          },
          "Adm": {
            "type": "boolean"
          }
        }
      },
      "Categoria": {
        "type": "object",
        "properties": {
          "CategoriaID": {
            "type": "integer"
          },
          "Nome": {
            "type": "string",
            "maxLength": 50
          },
          "Coeficiente": {
            "type": "integer"
          }
        }
      },
      "Banco": {
        "type": "object",
