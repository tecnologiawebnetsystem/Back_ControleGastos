          "DespesasID": {
            "type": "integer"
          },
          "UsuarioID": {
            "type": "integer"
          },
          "CategoriaID": {
            "type": "integer"
          },
          "Descricao": {
            "type": "string",
            "maxLength": 200
          },
          "Valor": {
            "type": "number",
            "format": "decimal"
          },
          "DataVencimento": {
            "type": "string",
            "format": "date"
          },
          "DataPagamento": {
            "type": "string",
            "format": "date"
          },
          "Parcelado": {
            "type": "boolean"
          },
