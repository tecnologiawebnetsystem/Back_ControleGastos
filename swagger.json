          "BancoID": {
            "type": "integer"
          },
          "UsuarioID": {
            "type": "integer"
          },
          "Nome": {
            "type": "string",
            "maxLength": 100
          },
          "Codigo": {
            "type": "string",
            "maxLength": 10
          },
          "Agencia": {
            "type": "string",
            "maxLength": 20
          },
          "Conta": {
            "type": "string",
            "maxLength": 20
          },
          "Pix": {
            "type": "string",
            "maxLength": 100
          }
        }
      },
      "Despesa": {
